        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Megatech.NAFSC.WPFApp"
        mc:Ignorable="d"
        Title="EMRTest" Height="300" Width="600">
    <Grid Margin="0,0,2,3">
        <Button x:Name="button" Click="button_Click" Content="Start" HorizontalAlignment="Left" Margin="116,36,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnStop" Click="btnStop_Click" Content="Stop" HorizontalAlignment="Left" Margin="116,76,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBox" Margin="0,154,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Height="40"/>
        <Button x:Name="btnGross" Click="btnGross_Click" Content="Gross" HorizontalAlignment="Left" Margin="116,113,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
